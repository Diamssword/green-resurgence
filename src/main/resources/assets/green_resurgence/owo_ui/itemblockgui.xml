<owo-ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/wisp-forest/owo-lib/1.20/owo-ui.xsd">
    <components>
<flow-layout direction="vertical" >
    <children>
        <flow-layout direction="horizontal"> <!---->
            <children>
                <flow-layout direction="vertical">
                    <children>
                        <label>
                            <horizontal-text-alignment>left</horizontal-text-alignment>
                            <text>Position</text>
                        </label>
                        <label>
                            <text>X</text>
                        </label>
                        <discrete-slider max="100" min="-100" id="posX">
                            <sizing>
                                <horizontal method="fill">100</horizontal>
                            </sizing>
                        </discrete-slider>
                        <label>
                            <text>Y</text>
                        </label>
                        <discrete-slider max="100" min="-100" id="posY">
                            <sizing>
                                <horizontal method="fill">100</horizontal>
                            </sizing>
                        </discrete-slider>
                        <label>
                            <text>Z</text>
                        </label>
                        <discrete-slider max="100" min="-100" id="posZ">
                            <sizing>
                                <horizontal method="fill">100</horizontal>
                            </sizing>
                        </discrete-slider>
                        <label>
                            <text>------------------------</text>
                        </label>
                        <label>
                            <horizontal-text-alignment>left</horizontal-text-alignment>
                            <text>Rotation</text>
                        </label>
                        <label>
                            <text>X</text>
                        </label>
                        <discrete-slider max="180" min="-180" id="rotX">
                            <sizing>
                                <horizontal method="fill">100</horizontal>
                            </sizing>
                        </discrete-slider>
                        <label>
                            <text>Y</text>
                        </label>
                        <discrete-slider max="180" min="-180" id="rotY">
                            <sizing>
                                <horizontal method="fill">100</horizontal>
                            </sizing>
                        </discrete-slider>
                        <label>
                            <text>Z</text>
                        </label>
                        <discrete-slider max="180" min="-180" id="rotZ">
                            <sizing>
                                <horizontal method="fill">100</horizontal>
                            </sizing>
                        </discrete-slider>
                    </children>
                    <horizontal-alignment>left</horizontal-alignment>
                    <vertical-alignment>top</vertical-alignment>

                    <sizing>
                        <vertical method="fill">100</vertical>
                        <horizontal method="fill">20</horizontal>

                    </sizing>
                    <!---->

                </flow-layout>
                <flow-layout direction="vertical">
                    <children>
                        <inventory id="player" name="disabled" width="9" height="3"/>
                        <inventory id="hotbar" width="9" height="1">
                            <margins>
                                <top>-5</top>

                            </margins>
                        </inventory>
                    </children>

                    <horizontal-alignment>center</horizontal-alignment>
                    <vertical-alignment>bottom</vertical-alignment>
                    <positioning type="relative">50,97</positioning>
                    <sizing>
                        <vertical method="fixed">80</vertical>
                        <horizontal method="fixed">200</horizontal>

                    </sizing>
                </flow-layout>
                <flow-layout direction="vertical">
                    <children>
                        <label>
                            <horizontal-text-alignment>left</horizontal-text-alignment>
                            <text>Taille</text>
                        </label>

                        <discrete-slider max="100" min="1" id="size">
                            <sizing>
                                <horizontal method="fill">100</horizontal>
                            </sizing>
                        </discrete-slider>
                        <label>
                            <text>------------------------</text>
                        </label>
                        <label>
                            <horizontal-text-alignment>left</horizontal-text-alignment>
                            <text>Item</text>
                        </label>

                        <inventory id="container" name="" width="1" height="1"/>
                        <label>
                            <horizontal-text-alignment>left</horizontal-text-alignment>
                            <margins><top>5</top><bottom>2</bottom></margins>
                            <text>Collision</text>
                        </label>
                        <small-checkbox id="collision"/>
                    </children>
                    <positioning type="relative">100,0</positioning>
                    <horizontal-alignment>left</horizontal-alignment>
                    <vertical-alignment>top</vertical-alignment>

                    <sizing>
                        <vertical method="fill">100</vertical>
                        <horizontal method="fill">20</horizontal>

                    </sizing>
                    <!---->

                </flow-layout>
            </children>


            <horizontal-alignment>left</horizontal-alignment>
            <vertical-alignment>top</vertical-alignment>

            <sizing>
                <vertical method="fill">100</vertical>
                <horizontal method="fill">100</horizontal>

            </sizing>
            <!---->
            <padding>
                <all>10</all>
            </padding>
            <margins>
                <all>1</all>
            </margins>

        </flow-layout>
    </children>
    <vertical-alignment>top</vertical-alignment>
    <horizontal-alignment>left</horizontal-alignment>
</flow-layout>

    </components>
</owo-ui>
