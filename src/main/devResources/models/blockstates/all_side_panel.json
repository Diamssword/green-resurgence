{
  "multipart": [
	{
      "apply": {
        "model": "green_resurgence:block/$sub/$id",
		"y": 0
      },
      "when": {"north": "true"}
    },
	{
      "apply": {
        "model": "green_resurgence:block/$sub/$id",
		"y": 180
      },
      "when": {"south": "true"}
    },
	{
      "apply": {
        "model": "green_resurgence:block/$sub/$id",
		"y": 90
      },
      "when": {"east": "true"}
    },
	{
      "apply": {
        "model": "green_resurgence:block/$sub/$id",
		"y": 270
      },
      "when": {"west": "true"}
    },
	{
      "apply": {
        "model": "green_resurgence:block/$sub/$id",
		"x": 90
      },
      "when": {"down": "true"}
    },
	{
      "apply": {
        "model": "green_resurgence:block/$sub/$id",
		"x": 270
      },
      "when": {"up": "true"}
    }
  ]
}