{
  "multipart": [
    {
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subplate_$color"
      }
    },
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subfeet_r",
		"y": 0
      },
      "when": {
		"AND":[
			{"north": "false"},
			{"west": "false"}
		]
      }
    },
    {
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subfeet_l",
		"y": 0
      },
      "when": {
		"AND":[
			{"north": "false"},
			{"east": "false"}
		]
      }
    },
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subfeet_r",
		"y": 180
      },
      "when": {
		"AND":[
			{"south": "false"},
			{"east": "false"}
		]
      }
    },
    {
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subfeet_l",
		"y": 180
      },
      "when": {
		"AND":[
			{"south": "false"},
			{"west": "false"}
		]
      }
    },	
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subdecor_l"
      },
      "when": {
		"AND":[
			{"north": "false"},
			{"east":"true"}
		]
      }
    },
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subdecor_r"
      },
      "when": {
		"AND":[
			{"north": "false"},
			{"west":"true"}
		]
      }
    },
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subdecor_l",
		"y":180
      },
      "when": {
		"AND":[
			{"south": "false"},
			{"west":"true"}
		]
      }
    },
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subdecor_r",
		"y":180
      },
      "when": {
		"AND":[
			{"south": "false"},
			{"east":"true"}
		]
      }
    },
	
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subdecor_l_empty",
		"y":90
      },
      "when": {
		"AND":[
			{"east": "false"},
			{"south":"true"}
		]
      }
    },
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subdecor_r_empty",
		"y":90
      },
      "when": {
		"AND":[
			{"east": "false"},
			{"north":"true"}
		]
      }
    },
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subdecor_l_empty",
		"y":270
      },
      "when": {
		"AND":[
			{"west": "false"},
			{"north":"true"}
		]
      }
    },
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subdecor_r_empty",
		"y":270
      },
      "when": {
		"AND":[
			{"west": "false"},
			{"south":"true"}
		]
      }
    },
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subdecor",
		"y": 0
      },
      "when": {
			"north": "false"
      }
    },
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subdecor",
		"y": 180
      },
      "when": {
			"south": "false"
      }
    },
	
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subfeet_outer_r",
		"y": 0
      },
      "when": {
		  "AND":[
			{"north_west":"false"},
			{"north":"true"},
			{"west":"true"}
		  ]
		 }
    },
	
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subfeet_outer_l",
		"y": 0
      },
      "when": {
		  "AND":[
			{"north_east":"false"},
			{"north":"true"},
			{"east":"true"}
		  ]
		 }
    },
	
	
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subfeet_outer_l",
		"y": 180
      },
      "when": {
		  "AND":[
			{"south_west":"false"},
			{"south":"true"},
			{"west":"true"}
		  ]
		 }
    },
	
	
	{
      "apply": {
        "model": "green_resurgence:block/diams/$id/$subfeet_outer_r",
		"y": 180
      },
      "when": {
		  "AND":[
			{"south_east":"false"},
			{"south":"true"},
			{"east":"true"}
		  ]
		 }
    }
  ]
}
